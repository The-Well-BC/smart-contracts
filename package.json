{
  "dependencies": {
    "@openzeppelin/contracts": "^4.1.0",
    "@truffle/hdwallet-provider": "1.2.3",
    "openzeppelin-2.5": "npm:@openzeppelin/contracts@v2.5"
  },
  "scripts": {
    "test-chain": "ganache-cli --defaultBalanceEther 90000000000000000000 -i 1337 -p 9545",
